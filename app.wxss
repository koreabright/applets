/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
page{  
  background-color: #f5f5f5;
  position: relative;
  height: 100%; 
} 
button[plain] {
  border: 0;
  padding: 0;
}
button[disabled]:not([type]) {
  margin-top: 150rpx;
  color: #fff;
  opacity: 0.5;
  background: #527AFF;
  width: 520rpx;
  height: 100rpx;
  border-radius: 100rpx;
}
